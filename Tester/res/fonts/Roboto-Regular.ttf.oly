#meta type=font

[[font_atlas]]
storage = "discard"
"font_size" = 96
"mag_filter" = "linear"
"min_filter" = "linear mipmap linear"
"generate_mipmaps" = true
"common_buffer_preset" = "common"

[font_face]
storage = "keep"
[[font_face.kerning]]
pair = [ "e", "U+78",]
dist = 150

[[font_face.kerning]]
pair = [ "H", "e",]
dist = -200
